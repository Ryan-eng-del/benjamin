{
  "extends": "@benjamin/typescript-config/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "~/server/*": [
        "./server/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "next.config.mjs",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "postcss.config.js",
    "../web-admin/providers/StoreBuilder.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}